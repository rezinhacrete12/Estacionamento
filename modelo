  public Modelo ( ){
    super( );
    this.NomeModelo = nome;
    this.PlacaModelo = placa;
    this.CorModelo = cor;
  }
}
public String getNome( ){
  return nome;
}
public void setNome(String nome){
  this.nome = nome;
}
public String getPlaca( ){
  return placa;
}
public void setPlac(String placa){
  this.placa = placa;
}
public String getCor( ){
  return cor;
}
public void setCor(String Cor){
  this.cor = cor;
}
